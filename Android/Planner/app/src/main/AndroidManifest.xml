<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gts.planner">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.AgendaActivity"
            android:label="@string/dashboard"
            android:launchMode="singleTop" />
        <activity android:name=".controller.AddTaskActivity" />
        <activity android:name=".controller.AddCourseActivity"/>
        <activity android:name=".controller.AddExamActivity"/>
    </application>;

</manifest>